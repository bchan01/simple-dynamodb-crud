{
    "Version": "2012-10-17",
    "Statement": [{
            "Effect": "Allow",
            "Action": "logs:CreateLogGroup",
            "Resource": "arn:aws:logs:us-east-1:160165339582:*"
        },
        {
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": [
                "arn:aws:logs:us-east-1:160165339582:log-group:/aws/lambda/simple-dax-reader:*"
            ]
        }
    ]
}